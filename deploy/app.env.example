# API configuration
DATABASE_URL=postgresql+asyncpg://${POSTGRES_USER:-namesmith_admin}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB:-namesmith_db}
OPENAI_API_KEY=<your-openai-api-key>
WHOISJSON_API_KEY=<your-whoisjson-api-key>
WHOAPI_API_KEY=<your-whoapi-api-key>

# Background workers
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Database bootstrap values (used by the postgres container)
POSTGRES_DB=namesmith_db
POSTGRES_USER=namesmith_admin
POSTGRES_PASSWORD=<your-password>
